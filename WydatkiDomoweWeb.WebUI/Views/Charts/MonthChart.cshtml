@using Highsoft.Web.Mvc.Charts
@model WydatkiDomoweWeb.WebUI.Models.ChartViewModel

@Scripts.Render("~/bundles/charts")

@{
    Highcharts highcharts = new Highcharts();
    highcharts.Chart = new Chart();
    highcharts.Title = new Title { Text = "Zestawienie miesięczne" };
    highcharts.XAxis = new List<XAxis> { new XAxis { Categories = Model.Category } };
    highcharts.Series = new List<Series>(Model.SeriesData);
}

@(Html.Highsoft().Highcharts(highcharts, "chartMonth"))